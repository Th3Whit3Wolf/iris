{
  "extends": "../tsconfig.base",
  "compilerOptions": {
    "esModuleInterop": true,
    "baseUrl": "./src",
    "paths": {
      "~/*": ["./*"]
    },
    "module": "commonjs",
    "target": "es5",
    "outDir": "dist",
    "noImplicitAny": false,
    "removeComments": true,
  },
  "compileOnSave": true,
  "include": ["src/**/*", "./knexfile.ts"],
  "exclude": ["node_modules"]
}
