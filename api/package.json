{
  "name": "iris-api",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "chalk": "^5.0.1",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "knex": "^1.0.7",
    "nodemon": "^2.0.16",
    "pg": "^8.7.3",
    "socket.io": "^4.5.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.51",
    "@swc/core": "^1.2.197",
    "@swc/jest": "^0.2.21",
    "@types/express": "^4.17.13",
    "@types/express-serve-static-core": "^4.17.29",
    "@types/jest": "27.4.x",
    "@types/node": "^17.0.41",
    "@types/socket.io": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "5.27.1",
    "@typescript-eslint/parser": "5.27.1",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "^28.1.1",
    "nodemon": "^2.0.16",
    "prettier": "^1.18.2",
    "supertest": "^6.2.3",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.8.1",
    "tsc-alias": "^1.4.1",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.7.3"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      "dist/",
      "coverage/"
    ],
    "watch": [
      "src/"
    ],
    "execMap": {
      "ts": "node -r ts-node/register"
    },
    "env": {
      "NODE_ENV": "development"
    },
    "ext": "js,json,ts"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  }
}
